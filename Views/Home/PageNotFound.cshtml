@{
}
@{
    ViewData["Title"] = "Page Not Found";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Page Not Found

    </title>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/home.css" />

</head>

<body>

    <div class="container-fluid mw-100 mh-100 ">



        <div class="d-flex justify-content-center mt-5">
            <img src="~/Assets/404-Page-image.png" class="img-fluid" />
        </div>
        <div class="d-flex justify-content-center mt-2 my-3 py-1">
            <h2 align="justify">Page Not Found</h2>
        </div>
        <div class="d-flex justify-content-center my-3 py-1">
            <h1 style="font-weight: 700" align="justify">Oops! &nbsp;</h1>
            <h1 style="font-weight: 500" align="justify"> page you were looking for doesn't exits.</h1>
        </div>
        <div class="d-flex justify-content-center my-3 py-1">
            <h3 style="color: #414141" align="justify">
                You may have misstyped the address or the page may have Moved .
            </h3>
        </div>
        <div class="d-flex justify-content-center mt-4 my-3 py-1">
            <button type="button" class="btn btn-warning  rounded-pill border-2 px-3 border-wrning" align="justify">
                Go To Homepage<img src="~/Assets/right-arrow.png" class="ms-3" />
            </button>
        </div>
    </div>

</body>


<script src="/js/bootstrap.bundle.min.js"></script>
